    "resolve-url-loader": "^3.1.0",
    "sass": "^1.15.2",
    "sass-loader": "^8.0.0",
    "semantic-ui-ember": "github:Semantic-Org/Semantic-UI-Ember",
    "vue": "^2.6.11",
